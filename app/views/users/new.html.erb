<h1 class="title"> Museum Sign Up</h1>

<%= form_for(@user) do |f| %>

<div>
    <%= f.label :email %>
    <%= f.email_field :email %>
</div>

<div>
    <%= f.label :user_name %>
    <%= f.text_field :user_name %>
</div>

<div>
    <%= f.label :password %>
    <%= f.password_field :password %>
</div>
 
    <%= f.submit "Sign Up" %>
<% end %>